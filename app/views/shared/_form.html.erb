<%= simple_form_for @adventure do |f| %>
<%= f.input :name %>
<%= f.input :number_of_guests %>
<%= f.input :price  %>
<%= f.input :languages  %>
<%= f.input :description  %>
<%= f.input :location  %>
<%= f.input :photo, as: :file %>
<%= f.submit "Add Adventure", class: "btn btn-primary btn-block" %>
<% end %>
